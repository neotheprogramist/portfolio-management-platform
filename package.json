{
  "name": "portfolio-management-platform",
  "description": "Portfolio Management Platform",
  "engines": {
    "node": "21.6.2"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/fastify/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint .",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.fastify",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "test.e2e": "playwright test",
    "update": "npx npm-check-updates -u",
    "update.check": "npx npm-check-updates",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.4.5",
    "@builder.io/qwik-city": "1.4.5",
    "@dnlup/fastify-traps": "3.0.2",
    "@fastify/compress": "7.0.0",
    "@fastify/static": "7.0.1",
    "@playwright/test": "1.42.1",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/blocks": "7.6.17",
    "@storybook/builder-vite": "7.6.17",
    "@storybook/html": "7.6.17",
    "@storybook/html-vite": "7.6.17",
    "@types/eslint": "8.56.5",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.11.24",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "autoprefixer": "10.4.18",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-plugin-qwik": "1.4.5",
    "fastify": "4.26.2",
    "fastify-plugin": "4.5.1",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "storybook": "7.6.17",
    "storybook-framework-qwik": "0.3.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "undici": "6.7.0",
    "vite": "5.1.4",
    "vite-tsconfig-paths": "4.3.1"
  },
  "dependencies": {
    "@wagmi/connectors": "4.1.14",
    "@wagmi/core": "2.6.5",
    "@web3modal/siwe": "4.0.11",
    "@web3modal/wagmi": "4.0.11",
    "jsonwebtoken": "9.0.2",
    "siwe": "2.1.4",
    "surrealdb.js": "0.11.0",
    "tailwind-merge": "2.2.1",
    "viem": "2.7.18"
  }
}
